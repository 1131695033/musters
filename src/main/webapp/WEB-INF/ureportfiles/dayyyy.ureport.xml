ple"></dataset-value></cell><cell expand="Down" name="H4" row="4" col="8"><cell-style font-size="9" forecolor="0,0,0" font-family="宋体" align="center" valign="middle"><left-border width="1" style="solid" color="0,0,0"/><right-border width="1" style="solid" color="0,0,0"/><top-border width="1" style="solid" color="0,0,0"/><bottom-border width="1" style="solid" color="0,0,0"/></cell-style><dataset-value dataset-name="dayyy" aggregate="group" property="BANKNAME" order="none" mapping-type="simple"></dataset-value></cell><cell expand="Down" name="I4" row="4" col="9"><cell-style font-size="9" forecolor="0,0,0" font-family="宋体" align="center" valign="middle"><left-border width="1" style="solid" color="0,0,0"/><right-border width="1" style="solid" color="0,0,0"/><top-border width="1" style="solid" color="0,0,0"/><bottom-border width="1" style="solid" color="0,0,0"/></cell-style><dataset-value dataset-name="dayyy" aggregate="group" property="cashchkBankAcct" order="none" mapping-type="simple"></dataset-value></cell><cell expand="Down" name="J4" row="4" col="10"><cell-style font-size="9" forecolor="0,0,0" font-family="宋体" align="center" valign="middle"><left-border width="1" style="solid" color="0,0,0"/><right-border width="1" style="solid" color="0,0,0"/><top-border width="1" style="solid" color="0,0,0"/><bottom-border width="1" style="solid" color="0,0,0"/></cell-style><dataset-value dataset-name="dayyy" aggregate="group" property="CASHCHKNAME" order="none" mapping-type="simple"></dataset-value></cell><row row-number="1" height="18"/><row row-number="2" height="18"/><row row-number="3" height="18"/><row row-number="4" height="19"/><column col-number="1" width="80"/><column col-number="2" width="80"/><column col-number="3" width="80"/><column col-number="4" width="80"/><column col-number="5" width="74"/><column col-number="6" width="74"/><column col-number="7" width="74"/><column col-number="8" width="74"/><column col-number="9" width="74"/><column col-number="10" width="74"/><datasource name="oracle数据源" type="buildin"><dataset name="dayyy" type="sql"><sql><![CDATA[	SELECT	
    	sysdate as "date",
          :uu as "uu",
			a.id AS "id",
             (select b.label
		      from sys_dict_type a, sys_dict_value b
		      where a.id = b.dict_type_id
		      and a.type = 'bank_code'		      
		      and b.value = a.cashchk_bank_code) bankName,
              (select b.label
		      from sys_dict_type a, sys_dict_value b
		      where a.id = b.dict_type_id
		      and a.type = 'fee_stat'		      
		      and b.value = a.fee_sort) feeName,
                 (select b.label
		      from sys_dict_type a, sys_dict_value b
		      where a.id = b.dict_type_id
		      and a.type = 'settle_mode'		      
		      and b.value = a.settle_mode) settleName,
               (select b.label
		      from sys_dict_type a, sys_dict_value b
		      where a.id = b.dict_type_id
		      and a.type = 'cashchk_stat'		     
		      and b.value = a.cashchk_stat) cashchkName,
		a.cashchk_no AS "cashchkNo",
		a.cashchk_emp_no AS "cashchkEmpNo",
		a.cashchk_amt AS "cashchkAmt",
		a.cashchk_date AS "cashchkDate",
		(select sum(t.rcv_amt) from a_pay_flow t
		    where t.cashchk_id = a.id) sum,
		(select count(t.id) from a_pay_flow t
		    where t.cashchk_id = a.id) countu,
		 (select b.login_name from sys_user b
		 where b.id = a.cashchk_emp_no
			) userName,
		a.cashchk_bank_code AS "cashchkBankCode",
		a.settle_note_no AS "settleNoteNo",
		a.settle_bank_code AS "settleBankCode",
		a.cashchk_bank_acct AS "cashchkBankAcct",
		a.account_no AS "accountNo",
		a.dispose_date AS "disposeDate",
		a.acct_stat_code AS "acctStatCode",
		a.arrive_date AS "arriveDate",
		a.acct_book_id AS "acctBookId",
		a.bank_note_no AS "bankNoteNo",
		a.settle_mode AS "settleMode",
		a.cashchk_stat AS "cashchkStat",
		a.fee_sort AS "feeSort",
		a.create_by AS "createBy",
		a.create_date AS "createDate",
		a.update_by AS "updateBy",
		a.update_date AS "updateDate",
		a.remarks AS "remarks",
		a.del_flag AS "delFlag"
		FROM
		  a_cashchk_flow a 
        where
        a.id in (:id)
		order by a.id]]></sql><field name="date"/><field name="uu"/><field name="id"/><field name="BANKNAME"/><field name="FEENAME"/><field name="SETTLENAME"/><field name="CASHCHKNAME"/><field name="cashchkNo"/><field name="cashchkEmpNo"/><field name="cashchkAmt"/><field name="cashchkDate"/><field name="SUM"/><field name="COUNTU"/><field name="USERNAME"/><field name="cashchkBankCode"/><field name="settleNoteNo"/><field name="settleBankCode"/><field name="cashchkBankAcct"/><field name="accountNo"/><field name="disposeDate"/><field name="acctStatCode"/><field name="arriveDate"/><field name="acctBookId"/><field name="bankNoteNo"/><field name="settleMode"/><field name="cashchkStat"/><field name="feeSort"/><field name="createBy"/><field name="createDate"/><field name="updateBy"/><field name="updateDate"/><field name="remarks"/><field name="delFlag"/><parameter name="id" type="List" default-value="0"/><parameter name="uu" type="String" default-value="0"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
