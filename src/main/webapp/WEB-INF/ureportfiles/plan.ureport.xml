		SELECT 
         sysdate as yy,
      	 :uu as uu,
		  t.id id,
		  t.cons_no consNo, 
		  (select a.cons_name from c_cons a
		    where t.cons_no = a.cons_no) consName, 
		  (select a.cons_addr from c_cons a
		    where t.cons_no = a.cons_no) consAddr,
          (select b.Mr_Sect_Name
            from r_Plan a, r_Sect b
            where a.Id = t.mr_plan_id
            and a.Mr_Sect_Id = b.Id) mrSectName,
		  t.amt_ym amtYm, 
		  t.mr_sn mrSn, 
		  t.mr_digit mrDigit,
		  (select a.asset_no from c_meter a
		    where t.meter_id = a.id) assetNo, 
		  t.last_mr_num lastMrNum, 
		  t.last_mr_gq lastMrGq, 
		  t.this_read thisRead, 
		  t.this_read_gq thisReadGq, 
		  t.actual_mode actualMode,
		  t.mr_status_code mrStatusCode,
		  (select b.label
		    from sys_dict_type a, sys_dict_value b
		    where a.id = b.dict_type_id
		    and a.type = 'mr_status_code'
		    and b.value = t.mr_status_code ) mrStatusName,
		  (select b.label
		    from sys_dict_type a, sys_dict_value b
		    where a.id = b.dict_type_id
		    and a.type = 'mr_mode'
		    and b.value = t.actual_mode ) mrModeName, 
		  t.mr_type_code mrTypeCode,
		  (select b.label
		    from sys_dict_type a, sys_dict_value b
		    where a.id = b.dict_type_id
		    and a.type = 'mr_type_code'
		    and b.value = t.mr_type_code ) mrTypeName,
		  t.excp_type_code excpTypeCode, 
		  (select b.label
		    from sys_dict_type a, sys_dict_value b
		    where a.id = b.dict_type_id
		    and a.type = 'mr_excp_type'
		    and b.value = t.excp_type_code) excpTypeName 
	FROM 
      r_data t
    WHERE
        t.mr_plan_id in (:mrPlanId)
    ORDER BY t.mr_sn
]]></sql><field name="YY"/><field name="UU"/><field name="ID"/><field name="CONSNO"/><field name="CONSNAME"/><field name="CONSADDR"/><field name="MRSECTNAME"/><field name="AMTYM"/><field name="MRSN"/><field name="MRDIGIT"/><field name="ASSETNO"/><field name="LASTMRNUM"/><field name="LASTMRGQ"/><field name="THISREAD"/><field name="THISREADGQ"/><field name="ACTUALMODE"/><field name="MRSTATUSCODE"/><field name="MRSTATUSNAME"/><field name="MRMODENAME"/><field name="MRTYPECODE"/><field name="MRTYPENAME"/><field name="EXCPTYPECODE"/><field name="EXCPTYPENAME"/><parameter name="uu" type="String" default-value="0"/><parameter name="mrPlanId" type="List" default-value="0"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
