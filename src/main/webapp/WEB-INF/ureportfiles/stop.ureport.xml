a.id AS "id",
  sysdate as "date",
          :uu as "uu",
		a.app_no AS "appNo",
		a.p_gas_stop_date AS "pGasStopDate",
		a.gas_stop_app_per AS "gasStopAppPer",
		(select name 
		   from sys_user ser
			 where ser.id = a.gas_stop_app_per) stopName,
		a.cons_no AS "consNo",
		a.gm_no AS "gmNo",
		a.gm_read AS "gmRead",
		a.stop_ntf_style AS "stopNtfStyle",
		a.notify_per AS "notifyPer",
		(select name 
		   from sys_user userss
			 where userss.id = a.notify_per) perName,
		a.notify_rst AS "notifyRst",
		a.notify_time AS "notifyTime",
		a.stop_rst AS "stopRst",
		a.owe_acct_info AS "oweAcctInfo",
		a.owe_amt AS "oweAmt",
		a.owe_month AS "oweMonth",
		a.overdue_time AS "overdueTime",
		a.cr_date AS "crDate",
		a.remind_num AS "remindNum",
		a.proc_Ins_Id AS "procInsId",
		(select T2.Cont_Chanl_No
		          from c_Cust_Contact T1, c_Cust_Cont_Chanl T2
		         where T1.Cust_Id = (select Cust_Id from c_cons s where s.cons_no=a.cons_no)
		           and T2.Contact_Id = T1.Id
		           and T2.Def_Flag = '1'
		           and T1.Contact_Prio = (select min(r.Contact_Prio) from c_Cust_Contact r where r.Cust_Id = T1.Cust_Id)
		           and Rownum = 1) telePhone,
		           
		  (select CONS_ADDR
		   from c_cons s where s.cons_no=a.cons_no
		  )    consAddrName ,
		   (select CONS_NAME
		   from c_cons s where s.cons_no=a.cons_no
		  )    consName1 ,
		   (select CONS_NARURE
		   from c_cons s where s.cons_no=a.cons_no
		  )    consNarureName ,
		a.org_no AS "orgNo",
		(select name 
		   from sys_office D
			 where D.id = a.org_no) orgNoName,
		a.gas_stop_date AS "gasStopDate",
		a.gas_stop_content AS "gasStopContent",
		a.operator AS "operator",
		a.create_by AS "createBy.id",
		a.create_date AS "createDate",
		a.update_by AS "updateBy.id",
		a.update_date AS "updateDate",
		a.remarks AS "remarks",
		a.del_flag AS "delFlag"
from  a_stop_dtl a
where  a.id in (:id)]]></sql><field name="id"/><field name="date"/><field name="uu"/><field name="appNo"/><field name="pGasStopDate"/><field name="gasStopAppPer"/><field name="STOPNAME"/><field name="consNo"/><field name="gmNo"/><field name="gmRead"/><field name="stopNtfStyle"/><field name="notifyPer"/><field name="PERNAME"/><field name="notifyRst"/><field name="notifyTime"/><field name="stopRst"/><field name="oweAcctInfo"/><field name="oweAmt"/><field name="oweMonth"/><field name="overdueTime"/><field name="crDate"/><field name="remindNum"/><field name="procInsId"/><field name="TELEPHONE"/><field name="CONSADDRNAME"/><field name="CONSNAME1"/><field name="CONSNARURENAME"/><field name="orgNo"/><field name="ORGNONAME"/><field name="gasStopDate"/><field name="gasStopContent"/><field name="operator"/><field name="createBy.id"/><field name="createDate"/><field name="updateBy.id"/><field name="updateDate"/><field name="remarks"/><field name="delFlag"/><parameter name="uu" type="String" default-value="1"/><parameter name="id" type="List" default-value="1"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
