		sysdate as yy,
		:uu as uu,
		  t.id id,
		  (select a.name from sys_office a
		     where a.id = t.org_no) orgNoName,
		  s.cons_no consNo,
		  s.cons_name consName,
		  s.cons_addr consAddr,
          t.rcvbl_ym rcvblYm,
		  NVL((select c.bank_code from c_bank_acct c
		    where c.cons_id = s.id and c.def_flag='1' and c.del_flag='0' and rownum=1),'') bankCode,
		  (select b.label from sys_dict_type a,sys_dict_value b
		         where a.id = b.dict_type_id
		         and a.type = 'bank_code'
		         and b.value = (select c.bank_code de from c_bank_acct c
		            where c.cons_id = s.id and c.def_flag='1' and c.del_flag='0' and rownum=1)) bankCodeName,
		   (select c.account_name from c_bank_acct c
		       where c.cons_id = s.id and c.def_flag='1' and c.del_flag='0' and rownum=1) accountName,
		   (select c.bank_acct from c_bank_acct c
		       where c.cons_id = s.id and c.def_flag='1' and c.del_flag='0' and rownum=1) bankAcct,
		   NVL(t.rcvbl_amt,0) rcvblAmt,
		   NVL(t.rcved_amt,0) rcvedAmt,
		   NVL(t.rcvbl_penalty,0) rcvblPenalty,
		   NVL(t.rcved_penalty,0) rcvedPenalty,
		   NVL(((t.rcvbl_amt+t.rcvbl_penalty)-(t.rcved_amt+t.rcved_penalty)),0) totalArrearage
		 FROM a_rcvbl_flow t, c_cons s
		 where 
		 	t.cons_no = s.cons_no
		   AND t.owe_amt > 0
		   AND t.fee_stat = '01'
		   AND t.charge_mode = '0203'
		   AND t.id in (:ids)]]></sql><field name="YY"/><field name="UU"/><field name="ID"/><field name="ORGNONAME"/><field name="CONSNO"/><field name="CONSNAME"/><field name="CONSADDR"/><field name="RCVBLYM"/><field name="BANKCODE"/><field name="BANKCODENAME"/><field name="ACCOUNTNAME"/><field name="BANKACCT"/><field name="RCVBLAMT"/><field name="RCVEDAMT"/><field name="RCVBLPENALTY"/><field name="RCVEDPENALTY"/><field name="TOTALARREARAGE"/><parameter name="uu" type="String" default-value="0"/><parameter name="ids" type="List" default-value="0"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
